{
    "name": "divido/merchant-sdk-guzzle-5",
    "type": "library",
    "authors": [
        {
            "name": "Neil McGibbon",
            "email": "code@neilmcgibbon.com"
        },
        {
            "name": "Mike Lovely",
            "email": "mike.lovely@divido.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:DividoFinancialServices/php-merchant-sdk.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Divido\\MerchantSDKGuzzle5\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Divido\\MerchantSDKGuzzle5\\Test\\": "tests/"
        },
        "classmap": [
            "tests"
        ]
    },
    "require": {
        "php": ">=5.6",
        "psr/http-message": "^1.0",
        "guzzlehttp/psr7": "^1.5",
        "divido/merchant-sdk": "dev-feature/remove-guzzle",
        "guzzlehttp/guzzle": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5",
        "kint-php/kint": "^2.1",
        "hamcrest/hamcrest-php": "^2.0",
        "mockery/mockery": "1.1.0",
        "phpdocumentor/reflection-docblock": "~2.0",
        "phpdocumentor/phpdocumentor": "2.9.0"
    }
}
